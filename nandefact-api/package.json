{
  "name": "nandefact-api",
  "version": "0.1.0",
  "description": "Backend API para ÑandeFact - facturación electrónica SIFEN Paraguay",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/ tests/",
    "lint:fix": "eslint src/ tests/ --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "sifen",
    "facturacion-electronica",
    "paraguay",
    "dnit"
  ],
  "author": "",
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.10.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-import-x": "^4.5.0",
    "prettier": "^3.4.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.18.0",
    "vitest": "^2.1.0"
  },
  "dependencies": {
    "@types/pdfkit": "^0.17.4",
    "bullmq": "^5.67.3",
    "cors": "^2.8.6",
    "express": "^5.2.1",
    "facturacionelectronicapy-kude": "^1.0.18",
    "facturacionelectronicapy-qrgen": "^1.0.9",
    "facturacionelectronicapy-setapi": "^1.0.34",
    "facturacionelectronicapy-xmlgen": "^1.0.277",
    "facturacionelectronicapy-xmlsign": "^1.0.28",
    "helmet": "^8.1.0",
    "ioredis": "^5.9.2",
    "multer": "^2.0.2",
    "pdfkit": "^0.17.2",
    "zod": "^4.3.6"
  }
}
